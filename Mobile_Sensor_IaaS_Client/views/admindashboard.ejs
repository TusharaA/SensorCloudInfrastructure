<!-- page content -->
<div class="right_col" role="main" ng-init="initiate_admin_dashborad()">
    <!-- top tiles -->
    <div class="row tile_count" ng-init="initializeBar()">
        <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
            <span class="count_top"><i class="fa fa-user"></i> Total Customers</span>
            <div class="count blue">{{users}}</div>
        </div>
        <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
            <span class="count_top"><i class="fa fa-wifi"></i> Total Sensors</span>
            <div class="count blue">{{sensors}}</div>
        </div>
        <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
            <span class="count_top"><i class="fa fa-sellsy"></i> Total Clusters</span>
            <div class="count blue">{{clusters}}</div>
        </div>
        <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
            <span class="count_top"><i class="fa fa-signal"></i> Active Sensors</span>
            <div class="count blue">{{activesensors}}</div>
        </div>
        <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
            <span class="count_top"><i class="fa fa-pause-circle-o"></i> Stopped Sensors</span>
            <div class="count blue">{{stoppedsensorscount}}</div>
        </div>
        <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
            <span class="count_top"><i class="fa fa-times"></i> Terminated Sensors</span>
            <div class="count blue">{{terminatedsensorscount}}</div>
        </div>
        <div class="col-md-2 col-sm-4 col-xs-6 tile_stats_count">
            <span class="count_top"><i class="fa fa-money"></i> Total Revenue</span>
            <div class="count green">${{totalRevenue | number:2}}</div>
        </div>

    </div>
    <!-- /top tiles -->

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="dashboard_graph">

                <div class="row x_title">
                    <div class="col-md-12">
                        <h3>Sensor Distribution</h3>
                    </div>
                </div>

                <div class="col-md-4 col-sm-4 col-xs-12">
                    <div class="x_title">
                        <h3>Sensor Usage</h3>
                        <div class="clearfix"></div>
                    </div>
                    <div id="placeholder33" style="height: 260px; display: none" class="demo-placeholder"></div>
                    <div style="width: 100%" >
                        <canvas id="doughnut" class="chart chart-doughnut"
                                chart-data="sensorcount" chart-labels="sensortypes">
                        </canvas>
                    </div>
                </div>
                <div class="col-md-4 col-sm-4 col-xs-12 bg-white">
                    <div class="x_title">
                        <h3>Details</h3>
                        <div class="clearfix"></div>
                    </div>
                    <div class="col-md-12 col-sm-12 col-xs-6">
                        <ul style="vertical-align: bottom">
                            <li ng-repeat="sensor in sensortypes">
                                <p>{{sensor}}</p>
                                <div class="">
                                    <div class="progress progress_sm" style="width: 76%;">
                                        <div class="progress-bar bg-green" role="progressbar" data-transitiongoal="80"></div>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-4 col-sm-4 col-xs-12">
                    <div class="x_title">
                        <h3>Clusters <small>vs</small> Sensors</h3>
                        <div class="clearfix"></div>
                    </div>
                    <div id="placeholder33" style="height: 260px; display: none" class="demo-placeholder"></div>
                    <div style="width: 100%;"  >
                        <canvas id="bar" class="chart chart-bar" height="100" width="100"  style="margin-top: 20px" chart-data="clustercount" chart-labels="clusternames" chart-series="series">
                        </canvas>
                    </div>
                </div>


                <div class="clearfix"></div>
            </div>
        </div>

    </div>
    <br />

    <div class="row">


        <div class="col-md-4 col-sm-4 col-xs-12">
            <div class="x_panel tile">
                <div class="x_title">
                    <h2>Active Sensor Details</h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li><a class="collapse-link">Manage <i class="fa fa-wrench"></i></a></li>
                    </ul>
                    <div class="clearfix"></div>
                </div>



                <div class="x_content">
                    <p ng-show="!activesensorlist.length" style="text-align: center">No Active sensors</p>
                    <div class="table-responsive" ng-show="activesensorlist.length">
                        <table class="table table-striped jambo_table bulk_action">
                            <thead>
                                <tr class="headings">
                                    <th class="column-title">Cluster</th>
                                    <th class="column-title">Sensor Id </th>
                                    <th class="column-title">Type </th>
                                </tr>
                            </thead>

                            <tbody>
                                <tr class="even pointer" ng-repeat="activesensor in activesensorlist">
                                    <td class=" ">{{activesensor.clustername}}</td>
                                    <td class=" ">{{activesensor.sensorid}}</td>
                                    <td class=" ">{{activesensor.type}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-md-4 col-sm-4 col-xs-12">
            <div class="x_panel tile">
                <div class="x_title">
                    <h2>Stopped Sensor Details</h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li><a class="collapse-link">Manage <i class="fa fa-wrench"></i></a></li>
                    </ul>
                    <div class="clearfix"></div>
                </div>



                <div class="x_content">
                    <p ng-show="!stoppedsensorlist.length" style="text-align: center">No Stopped sensors</p>
                    <div class="table-responsive" ng-show="stoppedsensorlist.length">
                        <table class="table table-striped jambo_table bulk_action">
                            <thead>
                                <tr class="headings">
                                    <th class="column-title">Cluster</th>
                                    <th class="column-title">Sensor Id </th>
                                    <th class="column-title">Type </th>
                                </tr>
                            </thead>

                            <tbody>
                                <tr class="even pointer" ng-repeat="stoppedsensor in stoppedsensorlist">
                                    <td class=" ">{{stoppedsensor.clustername}}</td>
                                    <td class=" ">{{stoppedsensor.sensorid}}</td>
                                    <td class=" ">{{stoppedsensor.type}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>

        <div class="col-md-4 col-sm-4 col-xs-12">
            <div class="x_panel tile">
                <div class="x_title">
                    <h2>Terminated Sensor Details</h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li><a class="collapse-link">Manage <i class="fa fa-wrench"></i></a></li>
                    </ul>
                    <div class="clearfix"></div>
                </div>



                <div class="x_content">
                    <p ng-show="!terminatedsensorlist.length" style="text-align: center">No terminated sensors</p>
                    <div class="table-responsive" ng-show="terminatedsensorlist.length">
                        <table class="table table-striped jambo_table bulk_action">
                            <thead>
                                <tr class="headings">
                                    <th class="column-title">Cluster</th>
                                    <th class="column-title">Sensor Id </th>
                                    <th class="column-title">Type </th>
                                </tr>
                            </thead>

                            <tbody>
                                <tr class="even pointer" ng-repeat="terminatedsensor in terminatedsensorlist">
                                    <td class=" ">{{terminatedsensor.clustername}}</td>
                                    <td class=" ">{{terminatedsensor.sensorid}}</td>
                                    <td class=" ">{{terminatedsensor.type}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>


                </div>
            </div>
        </div>
    </div>
</div>
<!-- /page content -->

