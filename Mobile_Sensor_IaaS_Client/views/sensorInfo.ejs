<div class="right_col" role="main" ng-init="getUserAllSensorDetails()">
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">
                    <h2>Manage Sensors </h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                            <ul class="dropdown-menu" role="menu">
                                <li><a href="#">Settings 1</a>
                                </li>
                                <li><a href="#">Settings 2</a>
                                </li>
                            </ul>
                        </li>
                        <li><a class="close-link"><i class="fa fa-close"></i></a>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>

                <div class="x_content">

                    <p>Select the sensor to manage by selecting the required sensor</p>

                    <div class="table-responsive">
                        <table class="table table-striped jambo_table bulk_action">
                            <thead>
                            <tr class="headings">
                                <th class="column-title">Id</th>
                                <th class="column-title">SensorCluster </th>
                                <th class="column-title">SensorId </th>
                                <th class="column-title">SensorType </th>
                                <th class="column-title">Status </th>
                            </tr>
                            </thead>

                            <tbody>
                            <tr class="even pointer" ng-repeat="sensor in sensorlist">
                                <td class=" ">{{sensor.index}}</td>
                                <td class=" ">{{sensor.SensorHubName}}</td>
                                <td class=" ">{{sensor.SensorId}}</td>
                                <td class=" ">{{sensor.SensorType}}</td>
                                <td class=" ">{{sensor.Status}}</td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>